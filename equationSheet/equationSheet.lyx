\begin_layout Standard
\paragraph_spacing onehalf
\noindent
\align block
To find the absolute acceleration of 
\emph on
P,
\begin_inset Formula 
\begin{equation}
\frac{d}{dt}(\dot{\mathbf{R}})=\ddot{\mathbf{R}}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\frac{d}{dt}[(\dot{\mathbf{\rho}})_{r}]=(\ddot{\mathbf{\rho}})_{r}+\mathbf{\omega\times}(\dot{\mathbf{\rho}})_{r}
\end{equation}

\end_inset


\begin_inset Formula 
\[
\frac{d}{dt}(\mathbf{\omega\times\rho})=\dot{\mathbf{\omega}}\mathbf{\times\rho}+\mathbf{\omega\times\dot{\rho}}...
\]

\end_inset


\begin_inset Formula 
\begin{equation}
...=\dot{\mathbf{\omega}}\mathbf{\times\rho}+\mathbf{\omega\times(\dot{\rho})}_{r}+\mathbf{\omega\times(\omega\times\rho)}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\paragraph_spacing onehalf
\noindent
\align block
Therefore,
\begin_inset Formula 
